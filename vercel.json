{
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NEXT_BUILD_TRACE": "0"
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./"
}